{
  "name": "@whfed/wh-utils",
  "version": "1.0.0",
  "description": "前端业务常用工具函数库",
  "main": "lib/utils.cjs.js",
  "module": "lib/utils.esm.js",
  "unpkg": "lib/utils.umd.js",
  "types": "./types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "bootstrap": "yarn || pnpm i",
    "deploy": "bash scripts/deploy.sh",
    "gc": "bash scripts/gc.sh",
    "prebuild": "npm run lint:fix",
    "serve:docs": "docsify serve ./docs --open",
    "generate:docs": "node ./scripts/docs.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue src",
    "build": "cross-env NODE_ENV=production node scripts/rollup.prod.js",
    "commit": "git-cz",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint-staged": "pretty-quick --staged",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "keywords": [
    "@whfed/wh-utils",
    "@whfed/wh-tools",
    "utils",
    "tools"
  ],
  "author": "Hyhello",
  "license": "MIT",
  "bugs": {
    "url": "https://code.cestc.cn/cestc_wh/data_center/web-frontEnd/utils/-/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://code.cestc.cn/cestc_wh/data_center/web-frontEnd/utils.git"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js|jsx|ts|tsx|vue}": [
      "eslint --ext .ts,.jsx,.ts,tsx",
      "prettier --write",
      "git add ."
    ]
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/generator": "^7.15.0",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/runtime": "^7.8.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^27.0.6",
    "chalk": "^4.1.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "del": "^6.0.0",
    "docsify-cli": "^4.4.3",
    "eslint": "7.5.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.1",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "jest-environment-jsdom": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "rollup": "^2.59.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-jest": "^27.0.7",
    "tslib": "*",
    "typescript": "^3.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "directories": {
    "lib": "lib"
  }
}
